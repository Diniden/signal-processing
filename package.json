{
  "name": "@diniden/signal-processing",
  "version": "1.0.0",
  "main": "dist/lib.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "dev": "runner run --no-docker",
    "clean": "runner clean",
    "start": "node dist/server",
    "build": "NODE_ENV=production tsc -d --emitDeclarationOnly --outDir dist/src --project ./tsconfig.json && runner build lib"
  },
  "keywords": [],
  "author": "Diniden",
  "license": "MIT",
  "devDependencies": {
    "@tarwich/runner": "1.3.0",
    "@types/d3-color": "1.2.2",
    "@types/d3-force": "1.2.1",
    "@types/d3-hierarchy": "1.1.6",
    "@types/d3-interpolate": "1.3.1",
    "@types/d3-scale": "2.1.1",
    "@types/d3-scale-chromatic": "1.3.1",
    "@types/node": "11.13.5",
    "babel-core": "6.26.3",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "1.7.4",
    "d3-scale": "3.0.0",
    "express": "4.17.1",
    "parcel-bundler": "1.11.0",
    "parcel-plugin-prettier": "github:tarwich/parcel-plugin-prettier",
    "parcel-plugin-simple-string": "github:diniden/parcel-plugin-simple-string",
    "prettier": "1.18.2",
    "sass": "1.14.3",
    "tslint": "5.18.0",
    "typescript": "3.1.6"
  },
  "description": "",
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "not IE > 10"
  ],
  "runner": {
    "sources": [
      {
        "name": "client",
        "entry": "test/index.html",
        "parcel": {
          "outDir": "dist/client",
          "hmr": false
        }
      },
      {
        "name": "server",
        "entry": "test/server.js",
        "run": true,
        "parcel": {
          "cacheDir": "./.cache/server",
          "outDir": "./dist/server",
          "outFile": "index.js",
          "target": "node",
          "minify": false,
          "hmr": false
        }
      },
      {
        "name": "lib",
        "entry": "src/index.ts",
        "parcel": {
          "outFile": "lib.js",
          "sourceMaps": true
        }
      }
    ]
  }
}
